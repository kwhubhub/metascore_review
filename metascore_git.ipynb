    "with open(r'●●●.txt', encoding='utf-8_sig') as f:\n",
    "    data1 = f.read()  \n",
    "    data1 = data1.lower()\n",
    "    data1 = data1.replace('.', '')\n",
    "    data1 = data1.replace(',', '')\n",
    "    data1 = data1.replace('(', '')\n",
    "    data1 = data1.replace(')', '')\n",
    "    data1 = data1.replace('!', '')\n",
    "    data1 = data1.replace('?', '')\n",
    "    data1 = data1.replace(';', '')\n",
    "    data1 = data1.replace('\"', '')\n",
    "    data1 = data1.replace(':', '')\n",
    "\n",
    "with open(r'●●●.txt', encoding='utf-8_sig') as f:\n",
    "    # ファイルの内容を読み出す\n",
    "    data2 = f.read()  \n",
    "    data2 = data2.lower() ,
    "    data2 = data2.replace('.', '')\n",
    "    data2 = data2.replace(',', '')\n",
    "    data2 = data2.replace('(', '')\n",
    "    data2 = data2.replace(')', '')\n",
    "    data2 = data2.replace('!', '')\n",
    "    data2 = data2.replace('?', '')\n",
    "    data2 = data2.replace(';', '')\n",
    "    data2 = data2.replace('\"', '')\n",
    "    data2 = data2.replace(':', '')\n",
    "\n",
    "with open(r'●●●.txt', encoding='utf-8_sig') as f:\n",
    "    data3 = f.read()  \n",
    "    data3 = data3.lower()\n",
    "    data3 = data3.replace('.', '')\n",
    "    data3 = data3.replace(',', '')\n",
    "    data3 = data3.replace('(', '')\n",
    "    data3 = data3.replace(')', '')\n",
    "    data3 = data3.replace('!', '')\n",
    "    data3 = data3.replace('?', '')\n",
    "    data3 = data3.replace(';', '')\n",
    "    data3 = data3.replace('\"', '')\n",
    "    data3 = data3.replace(':', '')\n",
    "\n",
    "with open(r'●●●.txt', encoding='utf-8_sig') as f:\n",
    "    data4 = f.read()  \n",
    "    data4 = data4.lower()\n",
    "    data4 = data4.replace('.', '')\n",
    "    data4 = data4.replace(',', '')\n",
    "    data4 = data4.replace('(', '')\n",
    "    data4 = data4.replace(')', '')\n",
    "    data4 = data4.replace('!', '')\n",
    "    data4 = data4.replace('?', '')\n",
    "    data4 = data4.replace(';', '')\n",
    "    data4 = data4.replace('\"', '')\n",
    "    data4 = data4.replace(':', '')\n",
    "\n",
    "with open(r'●●●.txt', encoding='utf-8_sig') as f:\n",
    "    data5 = f.read()  \n",
    "    data5 = data5.lower()\n",
    "    data5 = data5.replace('.', '')\n",
    "    data5 = data5.replace(',', '')\n",
    "    data5 = data5.replace('(', '')\n",
    "    data5 = data5.replace(')', '')\n",
    "    data5 = data5.replace('!', '')\n",
    "    data5 = data5.replace('?', '')\n",
    "    data5 = data5.replace(';', '')\n",
    "    data5 = data5.replace('\"', '')\n",
    "    data5 = data5.replace(':', '')\n",
    "\n",
    "# 単語カウント-------------------------\n",
    "# 単語を格納するリストを作成\n",
    "words1 = []\n",
    "words2 = []\n",
    "words3 = []\n",
    "words4 = []\n",
    "words5 = []\n",
    "\n",
    "# split()でスペースと改行で分割したリストから単語を取り出す\n",
    "for word1 in data1.split():\n",
    "    words1.append(word1)\n",
    "\n",
    "for word2 in data2.split():\n",
    "    words2.append(word2)\n",
    "\n",
    "for word3 in data3.split():\n",
    "    words3.append(word3)\n",
    "\n",
    "for word4 in data4.split():\n",
    "    words4.append(word4)\n",
    "\n",
    "for word5 in data5.split():\n",
    "    words5.append(word5)\n",
    "\n",
    "#出現頻度が300位までの単語を変数に格納\n",
    "c1 = collections.Counter(words1).most_common(300)\n",
    "c2 = collections.Counter(words2).most_common(300)\n",
    "c3 = collections.Counter(words3).most_common(300)\n",
    "c4 = collections.Counter(words4).most_common(300)\n",
    "c5 = collections.Counter(words5).most_common(300)\n",
    "\n",
    "#151位～300位のみ抽出\n",
    "c1_151_300 = c1[150:300]\n",
    "c2_151_300 = c2[150:300]\n",
    "c3_151_300 = c3[150:300]\n",
    "c4_151_300 = c4[150:300]\n",
    "c5_151_300 = c5[150:300]\n",
    "\n",
    "\n",
    "# 単語カウント-------------------------\n",
    "# 単語を数える辞書を作成\n",
    "words1d = {}\n",
    "words2d = {}\n",
    "words3d = {}\n",
    "words4d = {}\n",
    "words5d = {}\n",
    "\n",
    "for word1 in data1.split():\n",
    "    # .getで辞書にその単語が含まれていなければ0を返す。\n",
    "    words1d[word1] = words1d.get(word1, 0) + 1  \n",
    "\n",
    "for word2 in data2.split():\n",
    "    words2d[word2] = words2d.get(word2, 0) + 1  \n",
    "\n",
    "for word3 in data3.split():\n",
    "    words3d[word3] = words3d.get(word3, 0) + 1  \n",
    "\n",
    "for word4 in data4.split():\n",
    "    words4d[word4] = words4d.get(word4, 0) + 1  \n",
    "\n",
    "for word5 in data5.split():\n",
    "    words5d[word5] = words5d.get(word5, 0) + 1  \n",
    "\n",
    "\n",
    "#　kにはkeyが入り、vにはvalueが入る\n",
    "d1 = [ (v, k) for k, v in words1d.items()]\n",
    "d2 = [ (v, k) for k, v in words2d.items()]\n",
    "d3 = [ (v, k) for k, v in words3d.items()]\n",
    "d4 = [ (v, k) for k, v in words4d.items()]\n",
    "d5 = [ (v, k) for k, v in words5d.items()]\n",
    "\n",
    "# 単語の出現回数でソート\n",
    "d1.sort()\n",
    "d1.reverse()\n",
    "\n",
    "d2.sort()\n",
    "d2.reverse()\n",
    "\n",
    "d3.sort()\n",
    "d3.reverse()\n",
    "\n",
    "d4.sort()\n",
    "d4.reverse()\n",
    "\n",
    "d5.sort()\n",
    "d5.reverse()\n",
    "\n",
    "#　151位～300位の単語をデータフレーム化してcsvで出力\n",
    "df1 = pd.DataFrame(d1).set_index(0)\n",
    "df1 = df1.rename(columns={0:'count', 1:'word'})\n",
    "df1.to_csv('●●●.csv', encoding='utf-8_sig')\n",
    "\n",
    "df2 = pd.DataFrame(d2).set_index(0)\n",
    "df2 = df2.rename(columns={0:'count', 1:'word'})\n",
    "df2.to_csv('●●●.csv', encoding='utf-8_sig')\n",
    "\n",
    "df3 = pd.DataFrame(d3).set_index(0)\n",
    "df3 = df3.rename(columns={0:'count', 1:'word'})\n",
    "df3.to_csv('●●●.csv', encoding='utf-8_sig')\n",
    "\n",
    "df4 = pd.DataFrame(d4).set_index(0)\n",
    "df4 = df4.rename(columns={0:'count', 1:'word'})\n",
    "df4.to_csv('●●●.csv', encoding='utf-8_sig')\n",
    "\n",
    "df5 = pd.DataFrame(d5).set_index(0)\n",
    "df5 = df5.rename(columns={0:'count', 1:'word'})\n",
    "df5.to_csv('●●●.csv', encoding='utf-8_sig')"
   ]
